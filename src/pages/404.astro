---
import BaseLayout from '../layouts/BaseLayout.astro';
---
<BaseLayout title="Page not found | Freaky Flyer Delivery" description="The page you were looking for could not be found." pageType="article">
  <section class="section" aria-labelledby="notfound-heading">
    <div class="container" style="text-align:center;">
      <h1 id="notfound-heading">Oops! We could not find that page.</h1>
      <p>Double check the URL or head back to the home page.</p>
      <a class="button" href="/">Return home</a>
    </div>
  </section>
</BaseLayout>
